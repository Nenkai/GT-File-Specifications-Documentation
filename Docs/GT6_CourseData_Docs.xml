<!-- 
-------- GT6 Documentation for event course data ----------
------------- game_parameter/gt6/<folder> -----------------
-->

<xml>
	<GameParameter version="106">
		<championship value="0" />

		<!--
			File name of the current file, where value is the xxx in rxxx.xml
		-->
		<folder_id value="5000" />
		<events>
			<event>
				<constraint>
					<abs value="-1" />
					<active_steering value="-1" />
					<asm value="-1" />
					<drift_type value="-1" />
					<driving_line value="-1" />
					<enemy_tire value="-1" />
					<limit_tire_f value="-1" />
					<limit_tire_r value="-1" />
					<need_tire_f value="-1" />
					<need_tire_r value="-1" />
					<simulation value="-1" />
					<suggest_tire_f value="-1" />
					<suggest_tire_r value="-1" />
					<tcs value="-1" />
					<transmission value="-1" />
				</constraint>
				<entry_set>
					<!--
						Entry_generate can seemingly be removed, allowing for a fixed grid to be specified
						If a fixed grid is specified, the player will be assigned whichever vehicle lines up with the set player_pos
					-->
					<entry_generate>
						<delays />

						<!--
							Global AI skill values - unsure if overridden by individual ones
						-->
						<ai_skill value="90" />
						<ai_skill_breaking value="90" />
						<ai_skill_cornering value="90" />
						<ai_skill_accelerating value="90" />

						<!--
							Number of vehicles in the race, and the player's starting position
						-->
						<entry_num value="16" />
						<player_pos value="12" />
						<enemy_lv value="0" />
						<enemy_bspec_lv value="8" />

						<!--
							The way potential AI entries are picked from the entry_base_array below
							
							Generate types:
							NONE - No AI entries in the race (use for licence and time attacks)
							ENTRY_BASE_ORDER - AI entries will be assigned in the specific order of the array (so first entry is 1st place, second entry is 2nd etc) - number of which MUST match or exceed entry_num or will softlock on loading
							ENTRY_BASE_SHUFFLE - AI entries will be picked randomly from the array like a pool of entries
						-->
						<generate_type value="ENTRY_BASE_SHUFFLE" />

						<!--
							The order in which AI entries will appear on the starting grid_sort_type
							
							Sort types:
							PP_ASCEND - Lowest PP cars will be at the front
							PP_DESCEND - Lowest PP cars will be at the rear
						-->
						<enemy_sort_type value="NONE" />
						<use_rolling_start_param value="0" />
						<rolling_start_v value="0" />
						<gap_for_start_rolling_distance value="0" />
						<ai_skill_starting value="97" />
						<ai_roughness value="0" />

						<!-- 
							An array of (AI) entry parameters, one entry_base per potential entry
						-->
						<entry_base_array>
							<entry_base>

								<!--
									Optional - How far around the track, in metres (as with vcoord), the entrant will start
								-->
								<initial_position value="100" />

								<!--
									Optional - The speed (in km/h) at which the entrant will start
								-->
								<initial_velocity value="100" />

								<driver_name value="W. Armstrong" />

								<!--
									Country code for the driver's flag - set to anything invalid to remove flag
								-->
								<driver_region value="PD" />

								<!--
									How many seconds after "3, 2, 1" this AI will start
								-->
								<delay value="0" />

								<!--
									Color is the index of the vehicle's available colours (0 is default, potential colours can be found against each vehicle in VARIATION table of specdb)
									Label is the internal name of the vehicle the AI will use, from specdb
								-->
								<car color="0" label="ts030_hybrid_12" />

								<!--
									race_class_id is the "bucket" the AI racer will be in
									E.g. 6 cars assigned to race_class_id 0 and 6 assigned to race_class_id 1 will have 12 cars on track, but each bucket only competes against others within that bucket
								-->
								<race_class_id value="0" />

								<!--
									Individual values for AI skill
									From 0-200, but typically 0-100
									Values over 100 may be used to make AI extra aggressive/confident, although values too high will make them more mistake-prone or ignore braking points
								-->
								<ai_skill_breaking value="90" />
								<ai_skill_cornering value="90" />
								<ai_skill_accelerating value="90" />

								<!-- 
									Effects unknown, assumed to be AI aggression
									Most events are set to -1, but some are 0, 7, and 10
								-->
								<ai_roughness value="-1" />

								<!-- 
									The name of the tire compounds the AI will use
									Presumable AI that start with dry tires in a wet race will eventually try to pit and change them
								-->
								<tire_f value="RACING_HARD" />
								<tire_r value="RACING_HARD" />
							</entry_base>
						</entry_base_array>
					</entry_generate>
				</entry_set>
				<failure_condition>
					<type_list />
					<no_failure_at_result value="1" />
				</failure_condition>

				<!--
					Only present if a licence or Coffee Break 
				-->
				<license_condition>
					<failure_condition />
					<use_basic_finish value="0" />
					<stop_on_finish value="1" />

					<!--
						Display_mode determines which custom objective to show on screen
						
						Display mode types:
						DRIFT_SCORE - Player's drift score used for mission races
						FUEL_DIST - How far the current fuel amount will last - used for eco challenges*
						FUEL_TIME - How long the current fuel amount will last - used for echo challenges*
						PYLON_NUM - How many cones (or possibly all gadgets) have been hit - used for cone challenges*
						PYLON_TIME - How long the player has to hit the cones - used for cone challenges*
						
						* = Possibly only works if event set to COFFEE_BREAK
					-->
					<display_mode value="PYLON_NUM" />

					<gadget_names>
						<!-- 
							An array of the gadget (physics props) types used (labels from GADGETS in specdb) for PYLON_NUM scoring
						-->
						<gadget value="pylon_24h" />
					</gadget_names>

					<!-- 
						An array of conditions that will trigger the end of the event
					-->
					<finish_condition>
						<data>
							<!--
								The type of occurrence that will trigger the end
								
								Check types:
								BEST_LAP_TIME - The time of the current best lap
								COMPLETE_FLAG - Unknown
								COURSE_OUT - Player has left the course (unsure if bit or count of times left course)
								GADGET_COUNT - How many gadgets have been hit
								FUEL_AMOUNT - The amount of fuel left in the player's vehicle
								HIT_COUNT - How many times the player has collided with something
								HIT_POWER - How hard the player has collided with something (unsure of unit)
								HIT_WALL - If the player has hit a wall
								LAP_COUNT - How many laps have been completed
								LAP_TIME - The run time of the current lap
								ROAD_DISTANCE - Unknown (distance to finish?)
								STANDING_TIME - Unknown (how long the player has stood still?)
								TOTAL_TIME - The run time of the whole event
								V_POSITION - The player's current distance around the track (in metres)
								VELOCITY - The player's current speed (in km/h)
								WRONG_WAY_COUNT - How many times the player has gone the wrong way
							-->
							<check_type value="TOTAL_TIME" />

							<!--
								How the check_type should be compared to the value(s)
								
								Condition types:
								EQUAL - Equal to the value
								GREATER - Greater than the value
								GREATER_EQUAL - Greater than or equal to the value
								LESS - Less than the value
								LESS_EQUAL - Less than or equal to the value
								NOTEQUAL - Not equal to the value
							-->
							<condition value="GREATER_EQUAL" />

							<!--
								The logic against which the check_types are evaluated
								
								Connection types:
								AND - All of the specified checks are true
								OR - Any of the specified checks are true
								XOR - Only one of the specified checks is true
							-->
							<connection value="OR" />

							<!--
								Unknown, always set to 0
							-->
							<result_type value="0" />

							<!--
								The value being compared against by the checks above
								
								Value types:
								float_value - Floating point value (decimal)
								uint_value - Unsigned integer value (allows larger value but no negative value)
								int_value - Signed integer value (does not allow larger value but negative values allowed)
							-->
							<float_value value="0" />
							<uint_value value="30000" />
							<int_value value="0" />
						</data>
					</finish_condition>
					<success_condition>

						<!--
							An array of conditions that are checked when the finish is triggered
							Check types appear to be the same as those in finish_condition
							
							First = Gold, Second = Silver, Third = Bronze
						-->
						<data>
							<check_type value="GADGET_COUNT" />
							<condition value="GREATER_EQUAL" />
							<connection value="AND" />
							<result_type value="0" />
							<float_value value="0" />
							<uint_value value="180" />
							<int_value value="0" />
						</data>
						<data>
							<check_type value="GADGET_COUNT" />
							<condition value="GREATER_EQUAL" />
							<connection value="AND" />
							<result_type value="0" />
							<float_value value="0" />
							<uint_value value="140" />
							<int_value value="0" />
						</data>
						<data>
							<check_type value="GADGET_COUNT" />
							<condition value="GREATER_EQUAL" />
							<connection value="AND" />
							<result_type value="0" />
							<float_value value="0" />
							<uint_value value="100" />
							<int_value value="0" />
						</data>
					</success_condition>
				</license_condition>

				<!--
					The unique identifier of this race
					Connected to the array of event_ids in the main event XML
				-->
				<event_id value="5000001" />

				<!--
					Unknown, seems to always be set to race
				-->
				<event_type value="RACE" />

				<!--
					Game mode type
					Varies depending on the event, effects unknown as may depend on other variables set 
					
					Game mode types:
					EVENT_RACE - Typical GT Mode race
					EVENT_RALLY - Used for GT5 Rally Special Events - possibly triggers staggered entrant starts
					LICENSE - Licence test
					MISSION - Used for GT6 Mission Races - potentially sets time-based trophies similar to licences
					SCHOOL - Used for GT5 Special Events - possibly similar to Mission
					SINGLE_RACE - Possibly arcade-like races with no rewards
					TOUR - Used for GT5 Grand Tour - possibly similar to mission
				-->
				<game_mode value="EVENT_RACE" />
				<information>
					<advanced_notice />
					<flier_other_info value="" />
					<logo_other_info value="" />
					<race_info_minute value="0" />

					<!--
						Title is shown on the pre-race screen where entry restrictions/typical opponents are shown
						At top left near licence category
					-->
					<title>
						<JP>サンデーカートレース ジュニア : レース 1</JP>
						<US>Prototype Cup: Race 1</US>
						<GB>Prototype Cup: Race 1</GB>
						<FR>Courses de kart Junior Sunday : Course 1</FR>
						<DE>Sunday-"Kart Jr."-Rennen 1</DE>
						<IT>Gare amatori di Kart Junior: gara 1</IT>
						<ES>Carreras Sunday de karts Junior: Primera carrera</ES>
						<PT>Corridas Sunday de Karts Junior: Corrida 1</PT>
						<NL>Junioren Sunday kartraces: race 1</NL>
						<RU>Воскресный картинг Jr.: заезд 1</RU>
						<KR>선데이 카트 레이스 Jr. : 레이스 1</KR>
						<TW>假日盃小型賽車 Jr. ：第1戰</TW>
						<EL>Αγώνες Jr. Sunday Καρτ: 1ος Αγώνας</EL>
						<TR>Junior Sunday Go-Kart Yarışları: 1. Yarış</TR>
						<PL>Niedzielne wyścigi gokartów Junior: Wyścig 1</PL>
						<CZ>Junior Sunday - závody motokár: 1. závod</CZ>
						<HU>Junior Sunday gokart versenyek: 1. verseny</HU>
						<BP>Corridas de Kart Júnior Sunday: Corrida 1</BP>
						<MS>Carreras de Karting Sunday Junior: Carrera 1</MS>
					</title>

					<!-- 
						One_line_title is shown where the events are presented as bigger banners - e.g. GT6 Goodwood, licences, missions
					-->
					<one_line_title>
						<JP />
						<US />
						<GB />
						<FR />
						<DE />
						<IT />
						<ES />
						<PT />
						<NL />
						<RU />
						<KR />
						<TW />
						<EL />
						<TR />
						<PL />
						<CZ />
						<HU />
						<BP />
						<MS />
					</one_line_title>

					<!--
						Description is shown on the pre-race screen where entry restrictions/typical opponents are shown
						In details box below current player vehicle
					-->
					<description>
						<JP>グランツーリスモ レーシングカート ジュニアで参戦する、初心者向けレーシングカートレースです。レーシングカートならではのダイレクトなハンドリングを楽しもう！ レーシングカートはカーディーラー「グランツーリスモ」で購入できます。</JP>
						<US>An intense competition between prototype chassis racers.</US>
						<GB>An intense competition between prototype chassis racers.</GB>
						<FR>Une course de kart pour débutants. Seuls les karts Jr. sont autorisés. Prenez le volant et découvrez les sensations uniques du pilotage de kart ! Les karts peuvent être achetés à partir du concessionnaire Gran Turismo.</FR>
						<DE>Eine Rennkart-Veranstaltung für Anfänger. Man kann nur in einem Racing Kart Jr. teilnehmen. Erleben Sie das superdirekte Handling, das Ihnen nur ein Kart bieten kann! Rennkarts können beim "Gran Turismo"-Händler gekauft werden.</DE>
						<IT>Una gara di kart per principianti. Sono ammessi solo Kart Junior. Scendi in pista e goditi l'estrema maneggevolezza che solo un go-kart può garantire! Puoi acquistare i kart alla concessionaria Gran Turismo.</IT>
						<ES>Una carrera de karts para principiantes. Solo se puede participar con un kart Junior. ¡Sal ahí y disfruta de la conducción ultradirecta que solo puede ofrecerte un kart! Los karts de competición pueden adquirirse en el concesionario de Gran Turismo.</ES>
						<PT>Uma corrida de karts para principiantes. Aberta apenas a karts junior. Vai para a pista e aprecia a condução ultradireta que só um kart te dá! Podes comprar estes karts no concessionário do Gran Turismo. </PT>
						<NL>Een kartrace voor beginners, alleen voor de Jr.-kart. Ga het circuit op en geniet van de ultradirecte besturing die alleen karts te bieden hebben! Karts kunnen gekocht worden bij de Gran Turismo Dealers. </NL>
						<RU>Картинговые гонки для начинающих. Допускаются только карты класса Jr.. Выходите на трассу и испытайте чувство прямого управления машиной, которое дает только гоночный карт!</RU>
						<KR>그란 투리스모 레이싱 카트 Jr.로 도전하는 초보자를 위한 레이싱 카트 이벤트입니다. 레이싱 카트만이 주는 직관적인 핸들링을 맛보세요! 레이싱 카트는 카 딜러 [Gran Turismo]에서 구입할 수 있습니다.</KR>
						<TW>駕駛Gran Turismo 小型賽車Jr.參加，適合初學者的小型賽車競賽。請享受小型賽車特有的直覺式操作吧！小型賽車可在汽車經銷商「Gran Turismo」購買。</TW>
						<EL>Αγωνιστική διοργάνωση με καρτ για αρχάριους. Επιτρέπονται συμμετοχές μόνο σε Jr. καρτ. Βγες στην πίστα και απόλαυσε τον απίστευτα άμεσο χειρισμό που μόνο ένα καρτ μπορεί να προσφέρει! Μπορείς να αγοράσεις Αγωνιστικά Καρτ από την αντιπροσωπεία Gran Turismo.</EL>
						<TR>Yeni başlayanlar için bir go-kart yarış etkinliği. Yalnızca bir Jr. go-kart ile katılınabilir. Gelin ve sadece bir go-kartın sunabileceği üst düzey yol tutuşunun tadını çıkarın! Yarış go-kartları Gran Turismo bayiliğinden satın alınabilir.</TR>
						<PL>Wyścig gokartów dla początkujących. Można wziąć w nim udział tylko w gokarcie Junior. Poznaj niesamowitą sterowność, jaką charakteryzują się tylko gokarty! Gokarty zakupić można w salonie marki Gran Turismo.</PL>
						<CZ>Motokárový závod pro začátečníky. Vstoupit můžete pouze s motokárou třídy Jr. Vyrazte do boje a užijte si řízení v jeho ryzí podobě, které vám nabídne jedině motokára! Zakoupit si ji můžete v autosalónu Gran Turismo. </CZ>
						<HU>Gokartverseny kezdők számára, melyen kizárólag junior gokarttal lehet indulni. Irány a pálya, és élvezd a szuperközvetlen vezetési élményt, amit csak a gokart nyújthat! Versenygokartokat a Gran Turismo autókereskedésben vehetsz.</HU>
						<BP>Um evento de karts de corrida para iniciantes. Somente karts Jr. podem participar. Entre na pista e aproveite a dirigibilidade direta que somente um kart pode proporcionar! Karts de corrida podem ser comprados na concessionária Gran Turismo.</BP>
						<MS>Un evento de carreras de karting para principiantes. Solo se puede ingresar en un kart Jr. ¡Sal y disfruta de un manejo ultra directo que solo un kart puede ofrecer! Los karts pueden adquirirse en el concesionario de Gran Turismo.</MS>
					</description>
				</information>

				<!--
					Unknown, always set to 0 in existing events
				-->
				<inheritance value="0" />
				<race>
					<!--
						Possibly GT Academy related, possibly online-only
						Always set to 0 in existing events
					-->
					<academy_event value="0" />

					<!-- 
						Possibly related to GT Mode progression/award earning
						Usually set to 1 for A and B-Spec races, usually set to 0 for licence/specials
					-->
					<accumulation value="1" />

					<!--
						Used to toggle visible co-driver in rally events
						May depend on another parameter as setting this alone doesn't seem to have an effect
					-->
					<allow_codriver value="0" />

					<!--
						May be related to GT5 rally event staggered starts
						Set to 0 in every GT6 event, and every GT5 event outside of rally
					-->
					<auto_standing_delay value="0" />

					<!--
						Unknown effect, typically set to 0 or 1
					-->
					<autostart_pitout value="0" />

					<!--
						Vehicle damage effects
						
						Damage types:
						WEAK - Only visual damage (?)
						MIDDLE - Mechanical damage, may repair itself over time (?)
						STRONG - Mechanical damage
					-->
					<behavior_damage_type value="WEAK" />

					<!--
						Presumably slipstream strength
						Appears to always be set to GAME
					-->
					<behavior_slip_stream_type value="GAME" />

					<!--
						Unknown, possibly a bit for whether or not AI rubber banding is allowed
					-->
					<boost_type value="1" />
					<bspec_vitality10 value="10" />

					<!--
						How the race end is determined
						
						Complete types:
						BYLAPS - Ends after laps defined in race_limit_laps
						BYSECTION - Ends after crossing a defined section of track, most likely depends on event_goal_v being defined
						BYSTOP - Ends after stopping in zone - licences
						BYTIME - Triggers final lap when race_limit_minute is reached, ends after finishing final lap
						NONE - Appears to be used for drift events, maybe automatially determined by track sections
					-->
					<complete_type value="BYLAPS" />

					<!--
						Whether or not, and how quickly, tires and fuel are consumed
						In GT5 appears to be a bit - yes/no
						In GT6 appears to be a bit - most likely a multiplier (e.g. 5 = 5x faster), 0 = off
					-->
					<consume_fuel value="0" />
					<consume_tire value="0" />

					<!--
						Possibly used to set the starting time on tracks with dynamic time
					-->
					<datetime datetime="1970/00/00 00:00:00" />

					<!--
						Appears to be unused in GT6
						In GT5 uses SUNNY or RAINY to force that particular weather
					-->
					<decisive_weather value="NONE" />

					<!--
						Both apparently unused in GT Mode, presumably a bit - yes/no
					-->
					<disable_collision value="0" />
					<disable_recording_replay value="0" />

					<!--
						Enable damage and pits, presumably both bits - yes/no
						Unsure if disabling pit will cause any pit entry to be a drive-through
					-->
					<enable_damage value="1" />
					<enable_pit value="0" />

					<!--
						Possibly overrides the BYLAPS and/or BYTIME finish conditions so that custom conditions can be provided in finish_condition
					-->
					<endless value="0" />

					<!--
						The distance around the track and width of the custom finish line (in metres)
					-->
					<event_goal_v value="-1" />
					<event_goal_width value="-1" />

					<!--
						Possibly how the final rankings are determined
						
						Finish types:
						FASTEST - First to finish or fastest lap holder?
						TARGET - Finishing position (?)
					-->
					<finish_type value="TARGET" />
					<fixed_retention value="0" />

					<!--
						Unsure if racing flag rules or data flagset
					-->
					<flagset value="FLAGSET_NORMAL" />
					<ghost_presence_type value="NORMAL" />
					<ghost_type value="NONE" />
					<grid_sort_type value="NONE" />
					<immediate_finish value="0" />
					<initial_retention10 value="0" />
					<keep_load_ghost value="0" />
					<lighting_mode value="AUTO" />

					<!-- 
						Presumably track edge/wet surface friction type and grip percentage
					-->
					<low_mu_type value="MODERATE" />
					<mu_ratio100 value="100" />

					<online_on value="0" />

					<!--
						Whether or not rally event pace notes are used
						May depend on course generator track and rally parameters being set elsewhere
					-->
					<pace_note value="0" />

					<!--
						Penalties for cutting and/or hitting barriers/AI too hard
						Values unknown
					-->
					<penalty_level value="0" />
					<penalty_no_reset value="0" />

					<!-- 
						How many laps or minutes the race will last
						The one used is determined by complete_type
					-->
					<race_limit_laps value="6" />
					<race_limit_minute value="0" />

					<!--
						Possibly how the finish position is determined
						
						Race types:
						COMPETITION - Standard race
						DEMO - Unknown effects
						TIMEATTACK - By completion time, used for licences and mission races
					-->
					<race_type value="COMPETITION" />

					<!--
						How the entrants start the race
						Unknown if initial_position and initial_velocity override this
						
						Start types:
						GRID - Entrants are sat on the starting grid
						ROLLING - Single-file rolling start
						ROLLING_DL - Double-file rolling start
						ROLLING_NOLIMIT - Rolling start, presumably at max acceleration
					-->
					<start_type value="GRID" />

					<!-- 
						Multiplier of how fast time progresses (1.0 = real-time, 60.0 = 60x (i.e. 24 minute day))
					-->
					<time_progress_speed value="0.000000" />

					<!--
						Unknown - possibly how long in ms before the race starts, and how long the vehicles behind 1st place are given to finish?
					-->
					<time_to_finish value="3000" />
					<time_to_start value="6000" />

					<!--
						Temperature on track - unknown effect and whether or not temperature only works on tracks with time/weather progression
					-->
					<weather_base_celsius value="24" />
