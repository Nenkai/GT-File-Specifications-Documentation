                    -->
                    <weather_prec_rain_only value="1"/>
                    <weather_prec_snow_only value="0"/>
                    <!--
                        Forces the weather to be random or predetermined
                    -->
                    <weather_random value="1"/>
                    <weather_random_seed value="0"/>
                    <!--
