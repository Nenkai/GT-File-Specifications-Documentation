                    -->
                    <weather_total_sec value="180.000000"/>
                    <over_entry_max value="0"/>
                    <with_ghost value="0"/>
                    <replace_at_courseout value="0"/>
                    <!--
                        How fast weather and water can change
                    -->
                    <weather_accel10 value="10"/>
                    <weather_accel_water_retention10 value="10"/>
                    <boost_flag value="1"/>
                    <new_weather_data>
                        <point>
                            <time_rate value="0"/>
